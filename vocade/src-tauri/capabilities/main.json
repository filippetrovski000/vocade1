{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "main-capability",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:event:default",
    "core:window:default",
    "opener:default",
    "opener:allow-open-url",
    "opener:allow-default-urls",
    "deep-link:default",
    "deep-link:allow-register",
    "oauth:default"
  ]
} 